		<div id="page-content-wrapper">

			<div id="page-content-wrapper signin-page">

				<img class="blue-bg" src="img/bg-large.jpg">

				<!-- SIGN IN -->
				<div class="fade-in">
				<div class="page-content inset centered">
					<div style="margin: 0px;" class="row">

						<div class="col-md-6 col-md-offset-3">
							<img src="img/app-icon.png" alt="Insert Business Logo Here">
							<!--   src="img/logo-full-white.png"-->

							<h3 class="hidden-xs">Dry Cleaning &amp; Laundry delivered
								to your door</h3>

							<div class="fade-in">
								<div class="panel panel-orange"
									style="width: 600px; height: 330px">
									<div class="panel-heading">Login</div>
									<div class="panel-body pan">

										<form action="Washiz.do?submit=signin" method="post"
											class="form-horizontal">
											<div class="form-body pal">

												<div class="form-group" style="width: 600px">

													
														<label for="inputName">
															<%
																if (request.getAttribute("errmsg") != null) {
																	out.println(request.getAttribute("errmsg"));
																}
															%>
														</label>
													
												</div>



												<div class="form-group">
													<label for="inputName" class="col-md-3 control-label">
														Username</label>
													<div class="col-md-9">
														<div class="input-icon right">
															<i class="fa fa-user"></i> <input id="inputName"
																type="text" class="form-control" name="username" />
														</div>
													</div>
												</div>
												<div class="form-group">
													<label for="inputPassword" class="col-md-3 control-label">
														Password</label>
													<div class="col-md-9">
														<div class="input-icon right">
															<i class="fa fa-lock"></i> <input id="inputPassword"
																type="password" placeholder="" class="form-control"
																name="password" />
														</div>
													</div>
													<br> <br>
													<button data-ember-action="15" type="submit"
														id="login-submit" class="btn-right primary">Sign
														In</button>

												</div>

												<div class="form-group mbn">
													<div class="col-md-offset-3 col-md-6"></div>
												</div>
											</div>

										</form>
									</div>
								</div>
<a id="ember812" class="ember-view" href="forgetpassword.jsp">|Forgot Password ? |</a>



								</form>

							</div>
						</div>
					</div>
